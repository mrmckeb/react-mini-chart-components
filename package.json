{
  "name": "react-mini-chart-components",
  "version": "0.2.0",
  "description": "A collection of light-weight SVG mini chart components - with no dependencies.",
  "main": "index.js",
  "files": [
    "README.md",
    "LICENSE",
    "index.js",
    "lib/"
  ],
  "keywords": [
    "chart",
    "gauge",
    "graph"
  ],
  "scripts": {
    "build": "babel dev/src/ --out-dir lib/",
    "dev": "webpack-dev-server --config dev/.webpack.js --content-base dev/example --inline --hot",
    "lint": "eslint dev --ext .jsx --ext .js --max-warnings 0",
    "prepublish": "npm run lint && npm run test && npm run build",
    "test": "mocha dev/specs --compilers js:babel-register",
    "test-watch": "mocha -w dev/specs --compilers js:babel-register"
  },
  "author": "Brody McKee",
  "license": "GPL-3.0",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "eslint": "^2.5.1",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.3",
    "eventsource-polyfill": "^0.9.6",
    "mocha": "^2.4.5",
    "react": "^0.14.8",
    "react-addons-test-utils": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-transform-hmr": "^1.0.4",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "peerDependencies": {
    "react": "^0.14.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrmckeb/react-mini-chart-components"
  }
}
